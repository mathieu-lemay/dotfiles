<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.acidrain.setenv</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>
		launchctl setenv CARGO_HOME "${XDG_DATA_HOME:-${HOME}/.local/share}/cargo"
		launchctl setenv GOPATH "${XDG_DATA_HOME:-${HOME}/.local/share}/go"
		</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>

<!-- vim: set noet: -->
